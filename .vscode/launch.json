{
   // Use IntelliSense to find out which attributes exist for C# debugging
   // Use hover for the description of the existing attributes
   // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
   "version": "0.2.0",
   "configurations": [        
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
            "name": "Gateway",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build Gateway",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/Akkamart/Gateway/bin/Debug/netcoreapp2.2/Gateway.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Akkamart/Gateway",
            "stopAtEntry": false,
            "launchBrowser": {
                "enabled": true
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "OWN_HOST": "localhost",
                "SEED_NODE_HOST": "localhost",
                // "SEED_NODE_PORT": "8080",
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": "Membership",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build Membership",
            "program": "${workspaceFolder}/Akkamart/Memberships/bin/Debug/netcoreapp2.2/Memberships.dll",
            "args": ["Memberships.conf"],
            "cwd": "${workspaceFolder}/Akkamart/Memberships",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
                "OWN_HOST": "localhost",
                "SEED_NODE_HOST": "localhost",
                // "SEED_NODE_PORT": "8080",
            }
        },
        {
            "name": "Seed1",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build Seed1",
            //src/Seed/bin/Debug/netcoreapp2.2/Lighthouse.dll
            "program": "${workspaceFolder}/Akkamart/Seed1/bin/Debug/netcoreapp2.2/Seed1.dll",
            "args": ["seed1.conf"],
            "cwd": "${workspaceFolder}/Akkamart/Seed1",
            "stopAtEntry": false,
            "console": "integratedTerminal",
            "env": {
                "OWN_HOST": "localhost",
                "SEED_NODE_HOST": "localhost",
                // "SEED_NODE_PORT": "8080",
            }
        },
        {
            "name": "Seed2",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build Seed2",
            //src/Seed/bin/Debug/netcoreapp2.2/Lighthouse.dll
            "program": "${workspaceFolder}/Akkamart/Seed2/bin/Debug/netcoreapp2.2/Seed2.dll",
            "args": ["seed2.conf"],
            "cwd": "${workspaceFolder}/Akkamart/Seed2",
            "stopAtEntry": false,
            "console": "integratedTerminal",
            "requireExactSource": false,
            "env": {
                "OWN_HOST": "localhost",
                "SEED_NODE_HOST": "localhost",
                // "SEED_NODE_PORT": "8080",
            }
        },
        
    ]
}